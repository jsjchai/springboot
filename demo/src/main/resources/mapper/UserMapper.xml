<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jsjchai.springboot.demo.dao.UserDao">


    <select id="finaAll" resultType="com.jsjchai.springboot.demo.model.User">
        SELECT
            id,username,email
        FROM user
    </select>

    <select id="getById" resultType="com.jsjchai.springboot.demo.model.User">
        SELECT
            id,username,email
        FROM user
        WHERE id = #{id}
    </select>
</mapper>